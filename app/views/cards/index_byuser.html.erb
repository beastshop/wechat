
<div class="container">
<div class="well">
  <h3>祝福</h3>
</div>


<table class="table table-striped">
  <thead>
    <tr>
    <th>订单号</th>
    <th>内容</th>
    <th>图片</th>
  </tr>
  </thead>
  <tbody>
  <% @user.cards.each do |card| %>
    <tr>
      <td><%= card.order_no%>
        <a href="/cards/show_code?id=<%=card.id%>">二维码</a></td>
      <td><%= card.content %></td>
       <td><a href="<%= card.card_image.picture_file_name %>"><%= card.card_image.picture_file_name %></a></td>
    </tr>

  <% end %>
  </tbody>
</table>

</div>
