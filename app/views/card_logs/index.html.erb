<div class="well">
  <h3>祝福阅读日志</h3>
</div>
<table class="table table-striped">
  <thead>
    <tr>
    <th>订单号</th>
    <th>阅读时间</th>
    <th>ip</th>
    <th>brower</th>
    <th>角色查看</th>
  </tr>
</thead>
<tbody>
<% @card_logs.each do |card_log| %>
  <tr>
    <td><%= card_log.card.order_no %></td>
    <td><%= card_log.created_at %></td>
    <td><%= card_log.host %></td>
    <td><%= card_log.brower %></td>
    <td><%=raw(card_log.is_admin_read ? "Admin" : "User" ) %></td>
  </tr>
<% end %>
</tbody>
</table>


<%= paginate @card_logs %>